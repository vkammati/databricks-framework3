{
  "name": "{{ dv_workflow_name }}",
  "email_notifications": {
    "on_failure": [
        "GX-SBOBNG-DLT-Team@shell.com"
      ],
    "no_alert_for_skipped_runs": false
  },
  "max_concurrent_runs": 1,
  "schedule": {
            "quartz_cron_expression": "32 0 18 1 * ?",
            "timezone_id": "UTC",
            "pause_status": "PAUSED"
        },
  "format": "MULTI_TASK",
  "queue": {
      "enabled": true
  },
  "tags": {
      "workflow_project": "DATTA-TECH-EH-DV-WF"
    },
    "tasks": [
    {
      "task_key": "process_start_workflow",
      "notebook_task": {
        "notebook_path": "{{ repos_path }}/datta_tech_eh/process_start_workflow",
        "base_parameters": {
          "env": "{{ env }}",
          "repos_path": "{{ repos_path }}",
          "workflow_frequency":"DV",
          "unique_repo_branch_id": "",
          "unique_repo_branch_id_schema": ""
        },
        "source": "WORKSPACE"
      },
      "job_cluster_key": "sede-x-DATTA-TECH-EH-data_validation-workflow_job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {}
    }, 
    {
      "task_key": "data_validation_logic",
       "depends_on": [
        {
          "task_key": "process_start_workflow"
        }
      ],
      "notebook_task": {
        "notebook_path": "{{ repos_path }}/datta_pipeline_library/transformation/euh_eh/dataValidation_logic",
        "base_parameters": {
          "env": "{{ env }}",
          "repos_path": "{{ repos_path }}",
          "unique_repo_branch_id": "",
          "unique_repo_branch_id_schema": ""
        },
        "source": "WORKSPACE"
      },
      "job_cluster_key": "sede-x-DATTA-TECH-EH-data_validation-workflow_job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {}
    },
    {
      "task_key": "process_end_workflow",
      "depends_on": [
        {
          "task_key": "data_validation_logic"
        }
      ],
      "notebook_task": {
        "notebook_path": "{{ repos_path }}/datta_tech_eh/process_end_workflow",
        "base_parameters": {
          "env": "{{ env }}",
          "repos_path": "{{ repos_path }}",
          "workflow_frequency":"DV",
          "unique_repo_branch_id": "",
          "unique_repo_branch_id_schema": ""
        },
        "source": "WORKSPACE"
      },
      "job_cluster_key": "sede-x-DATTA-TECH-EH-data_validation-workflow_job_cluster",
      "timeout_seconds": 0,
      "email_notifications": {}
    }
  ],
  "job_clusters": [
      {
        "job_cluster_key": "sede-x-DATTA-TECH-EH-data_validation-workflow_job_cluster",
        "new_cluster": {
          "cluster_name": "",
          "spark_version": "15.4.x-scala2.12",
          "spark_conf": {
            "spark.databricks.delta.preview.enabled": "true"
          },
          "azure_attributes": {
            "first_on_demand": 1,
            "availability": "ON_DEMAND_AZURE",
            "spot_bid_max_price": -1
          },
          "node_type_id": "Standard_F16s_v2",
          "driver_node_type_id": "Standard_F64s_v2",
          "custom_tags": {
            "Name": "DATTA-TECH-EH-dataValidation-logic-WF-JC",
            "Environment": "dev"
          },
          "spark_env_vars": {
            "PYSPARK_PYTHON": "/databricks/python3/bin/python3"
          },
          "enable_elastic_disk": true,
          "data_security_mode": "USER_ISOLATION",
          "runtime_engine": "STANDARD",
          "autoscale": {
            "min_workers": 8,
            "max_workers": 12
          }
        }
      }
    ],
  "timeout_seconds": 0
}